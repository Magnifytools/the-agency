# The Agency - Environment Variables
# Copy to .env and fill in values

# Database (PostgreSQL) - Railway provides DATABASE_URL automatically
DATABASE_URL=postgresql+asyncpg://agency:agency@localhost:5432/the_agency

# Security - CHANGE THIS in production!
SECRET_KEY=generate-a-strong-random-key-here

# JWT token expiration (minutes)
ACCESS_TOKEN_EXPIRE_MINUTES=480

# Discord webhook (optional)
DISCORD_WEBHOOK_URL=

# Holded ERP API key (optional â€” enables finance sync)
HOLDED_API_KEY=

# Railway sets PORT automatically
PORT=8004
